<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_weight="0"

    android:padding="10dp"
    tools:context=".Paper2020">

    <androidx.cardview.widget.CardView

        android:id="@+id/card1Set1y2020"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardBackgroundColor="#EFB5B1"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="20dp"
app:cardCornerRadius="20dp"
        tools:ignore="MissingConstraints">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layoutSet1y2020"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="10dp">

            <de.hdodenhof.circleimageview.CircleImageView
                xmlns:app="http://schemas.android.com/apk/res-auto"
                app:civ_border_width="2dp"
                android:scaleType="centerCrop"
                android:id="@+id/ImageSet1y2020"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:src="@drawable/s"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"

                />
            <TextView

                android:id="@+id/TextSet1y2020"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="            Set 1"
                android:textSize="25dp"
                android:layout_marginLeft="10dp"
                android:textStyle="bold"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toRightOf="@id/ImageSet1y2020"
                app:layout_constraintRight_toRightOf="parent"/>

            <Button
                android:id="@+id/Set1y2020"
                android:layout_width="82dp"
                android:layout_height="35dp"
                android:layout_marginLeft="70dp"

                android:layout_marginTop="10dp"
                android:text="paper ->"
                app:layout_constraintLeft_toRightOf="@id/ImageSet1y2020"
                app:layout_constraintTop_toBottomOf="@id/TextSet1y2020" />
            <Button
                android:id="@+id/AnsSet1y2020"
                android:layout_width="82dp"
                android:layout_height="35dp"
                android:text="ans"
                android:layout_marginLeft="2dp"
                android:layout_marginTop="10dp"
                app:layout_constraintLeft_toRightOf="@+id/Set1y2020"
                app:layout_constraintTop_toBottomOf="@+id/TextSet1y2020"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>




    <androidx.cardview.widget.CardView
        android:id="@+id/card2Set2y2020"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardBackgroundColor="#BEEAB9"
        app:cardCornerRadius="20dp"
        app:layout_constraintTop_toBottomOf="@id/card1Set1y2020"
        android:layout_marginTop="20dp"
        tools:ignore="MissingConstraints">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layoutSet2y2020"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="10dp">

            <de.hdodenhof.circleimageview.CircleImageView
                xmlns:app="http://schemas.android.com/apk/res-auto"
                app:civ_border_width="2dp"
                android:scaleType="centerCrop"
                android:id="@+id/ImageSet2y2020"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:src="@drawable/s"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"

                />
            <TextView
                android:textStyle="bold"
                android:id="@+id/TextSet2y2020"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="            Set 2"
                android:textSize="25dp"
                android:layout_marginLeft="10dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toRightOf="@id/ImageSet2y2020"
                app:layout_constraintRight_toRightOf="parent"/>

            <Button
                android:id="@+id/openSet2y2020"
                android:layout_width="82dp"
                android:layout_height="35dp"
                android:layout_marginLeft="70dp"

                android:layout_marginTop="10dp"
                android:text="Paper ->"
                app:layout_constraintLeft_toRightOf="@id/ImageSet2y2020"
                app:layout_constraintTop_toBottomOf="@id/TextSet2y2020" />
            <Button
                android:id="@+id/ansSet1y2020"
                android:layout_width="82dp"
                android:layout_height="35dp"
                android:text="ans"
                android:layout_marginLeft="2dp"
                android:layout_marginTop="10dp"
                app:layout_constraintLeft_toRightOf="@+id/openSet2y2020"
                app:layout_constraintTop_toBottomOf="@+id/TextSet2y2020"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:id="@+id/card3Set3y2020"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardBackgroundColor="#EFB5B1"
        app:cardCornerRadius="20dp"
        app:layout_constraintTop_toBottomOf="@id/card2Set2y2020"
        android:layout_marginTop="20dp"
        tools:ignore="MissingConstraints">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="10dp">

            <de.hdodenhof.circleimageview.CircleImageView
                xmlns:app="http://schemas.android.com/apk/res-auto"
                app:civ_border_width="2dp"
android:scaleType="centerCrop"
                android:id="@+id/ImageSet3y2020"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:src="@drawable/s"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"

                />
            <TextView
                android:textStyle="bold"
                android:id="@+id/TextSet3y2020"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="            Set 3"
                android:textSize="25dp"
                android:layout_marginLeft="10dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toRightOf="@id/ImageSet3y2020"
                app:layout_constraintRight_toRightOf="parent"/>

            <Button
                android:id="@+id/openSet3y2020"
                android:layout_width="82dp"
                android:layout_height="35dp"
                android:layout_marginLeft="70dp"

                android:layout_marginTop="10dp"
                android:text="paper ->"
                app:layout_constraintLeft_toRightOf="@id/ImageSet3y2020"
                app:layout_constraintTop_toBottomOf="@id/TextSet3y2020" />
            <Button
                android:id="@+id/ansSet3y2020"
                android:layout_width="82dp"
                android:layout_height="35dp"
                android:text="ans"
                android:layout_marginLeft="2dp"
                android:layout_marginTop="10dp"
                app:layout_constraintLeft_toRightOf="@+id/openSet3y2020"
                app:layout_constraintTop_toBottomOf="@+id/TextSet3y2020"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/card4Set4y2020"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/card3Set3y2020"
        app:cardCornerRadius="20dp"
        android:layout_marginTop="20dp"
        app:cardBackgroundColor="#BEEAB9"
        tools:ignore="MissingConstraints">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layoutSet4y2020"

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="10dp">

            <de.hdodenhof.circleimageview.CircleImageView
                xmlns:app="http://schemas.android.com/apk/res-auto"
                app:civ_border_width="2dp"
                android:scaleType="centerCrop"
                android:id="@+id/ImageSet4y2020"
                android:layout_width="80dp"
                android:layout_height="80dp"

                android:src="@drawable/s"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"

                />
            <TextView
                android:textStyle="bold"
                android:id="@+id/TextSet4y2020"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="            Set 4"
                android:textSize="25dp"
                android:layout_marginLeft="10dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toRightOf="@id/ImageSet4y2020"
                app:layout_constraintRight_toRightOf="parent"/>

            <Button
                android:id="@+id/openSet4y2020"
                android:layout_width="82dp"
                android:layout_height="35dp"
                android:layout_marginLeft="70dp"

                android:layout_marginTop="10dp"
                android:text="paper ->"
                app:layout_constraintLeft_toRightOf="@id/ImageSet4y2020"
                app:layout_constraintTop_toBottomOf="@id/TextSet4y2020" />
            <Button
                android:id="@+id/ansSet4y2020"
                android:layout_width="82dp"
                android:layout_height="35dp"
                android:text="ans"
                android:layout_marginLeft="2dp"
                android:layout_marginTop="10dp"
                app:layout_constraintLeft_toRightOf="@+id/openSet4y2020"
                app:layout_constraintTop_toBottomOf="@+id/TextSet4y2020"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>