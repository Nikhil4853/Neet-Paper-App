<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="10dp"
    tools:context=".Paper2023">

    <androidx.cardview.widget.CardView

        android:id="@+id/card1E"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardBackgroundColor="#EFB5B1"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="20dp"
        app:cardCornerRadius="20dp"
        tools:ignore="MissingConstraints">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layoutE"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="10dp">

            <de.hdodenhof.circleimageview.CircleImageView
                xmlns:app="http://schemas.android.com/apk/res-auto"
                app:civ_border_width="2dp"
                android:id="@+id/ImageE"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:src="@drawable/s"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"

                />
            <TextView

                android:id="@+id/TextE"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="           E"
                android:textSize="25dp"
                android:layout_marginLeft="10dp"
                android:textStyle="bold"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toRightOf="@id/ImageE"
                app:layout_constraintRight_toRightOf="parent"/>

            <Button
                android:id="@+id/openE"
                android:layout_width="82dp"
                android:layout_height="35dp"
                android:layout_marginLeft="70dp"

                android:layout_marginTop="10dp"
                android:text="Open"
                app:layout_constraintLeft_toRightOf="@id/ImageE"
                app:layout_constraintTop_toBottomOf="@id/TextE" />


        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardF"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardBackgroundColor="#BEEAB9"
        app:cardCornerRadius="20dp"
        app:layout_constraintTop_toBottomOf="@id/card1E"
        android:layout_marginTop="20dp"
        tools:ignore="MissingConstraints">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layoutSet2y2020"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="10dp">

            <de.hdodenhof.circleimageview.CircleImageView
                xmlns:app="http://schemas.android.com/apk/res-auto"
                app:civ_border_width="2dp"
                android:id="@+id/ImageF"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:src="@drawable/s"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"

                />
            <TextView
                android:textStyle="bold"
                android:id="@+id/TextF"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="            F"
                android:textSize="25dp"
                android:layout_marginLeft="10dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toRightOf="@id/ImageF"
                app:layout_constraintRight_toRightOf="parent"/>

            <Button
                android:id="@+id/openF"
                android:layout_width="82dp"
                android:layout_height="35dp"
                android:layout_marginLeft="70dp"

                android:layout_marginTop="10dp"
                android:text="Open"
                app:layout_constraintLeft_toRightOf="@id/ImageF"
                app:layout_constraintTop_toBottomOf="@id/TextF" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:id="@+id/cardG"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardBackgroundColor="#EFB5B1"
        app:cardCornerRadius="20dp"
        app:layout_constraintTop_toBottomOf="@id/cardF"
        android:layout_marginTop="20dp"
        tools:ignore="MissingConstraints">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="10dp">

            <de.hdodenhof.circleimageview.CircleImageView
                xmlns:app="http://schemas.android.com/apk/res-auto"
                app:civ_border_width="2dp"
                android:id="@+id/ImageG"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:src="@drawable/s"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"

                />
            <TextView
                android:textStyle="bold"
                android:id="@+id/TextG"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="            G"
                android:textSize="25dp"
                android:layout_marginLeft="10dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toRightOf="@id/ImageG"
                app:layout_constraintRight_toRightOf="parent"/>

            <Button
                android:id="@+id/openG"
                android:layout_width="82dp"
                android:layout_height="35dp"
                android:layout_marginLeft="70dp"

                android:layout_marginTop="10dp"
                android:text="Open"
                app:layout_constraintLeft_toRightOf="@id/ImageG"
                app:layout_constraintTop_toBottomOf="@id/TextG" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardH"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/cardG"
        app:cardCornerRadius="20dp"
        android:layout_marginTop="20dp"
        app:cardBackgroundColor="#BEEAB9"
        tools:ignore="MissingConstraints">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layoutH"

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="10dp">

            <de.hdodenhof.circleimageview.CircleImageView
                xmlns:app="http://schemas.android.com/apk/res-auto"
                app:civ_border_width="2dp"
                android:id="@+id/ImageH"
                android:layout_width="80dp"
                android:layout_height="80dp"

                android:src="@drawable/s"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"

                />
            <TextView
                android:textStyle="bold"
                android:id="@+id/TextH"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="            H"
                android:textSize="25dp"
                android:layout_marginLeft="10dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toRightOf="@id/ImageH"
                app:layout_constraintRight_toRightOf="parent"/>

            <Button
                android:id="@+id/openH"
                android:layout_width="82dp"
                android:layout_height="35dp"
                android:layout_marginLeft="64dp"

                android:layout_marginTop="4dp"
                android:text="Open"
                app:layout_constraintLeft_toRightOf="@id/ImageH"
                app:layout_constraintTop_toBottomOf="@id/TextH" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>