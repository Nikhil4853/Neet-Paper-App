        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.cardview.widget.CardView
            android:id="@+id/card2023"
            app:cardBackgroundColor="#BEEAB9"
            app:cardCornerRadius="10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            tools:ignore="MissingConstraints">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/layout_card_2023"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp">

                <de.hdodenhof.circleimageview.CircleImageView
                    xmlns:app="http://schemas.android.com/apk/res-auto"
                    app:civ_border_width="2dp"
                    android:id="@+id/image2023"
                    android:layout_width="100dp"
                    android:layout_height="80dp"
                    android:src="@drawable/s"

                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"

                    />
                <TextView

                    android:id="@+id/year2023"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="            2023"
                    android:textSize="25dp"
                    android:layout_marginLeft="10dp"

                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintLeft_toRightOf="@id/image2023"
                    app:layout_constraintRight_toRightOf="parent"/>

                <Button
                    android:id="@+id/btn2023"
                    android:layout_width="wrap_content"
                    android:layout_height="35dp"
                    android:layout_marginLeft="80dp"

                    android:layout_marginTop="10dp"
                    android:text="Open"
                    app:layout_constraintLeft_toRightOf="@id/image2023"
                    app:layout_constraintTop_toBottomOf="@id/year2023" />
            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.cardview.widget.CardView>



